{
    "name": "Enlarge Profile Image for YouTubeâ„¢",
    "description": "A chrome extension that makes it possible to enlarge the profile image in YouTube profiles by clicking on it.",
    "manifest_version": 2,
    "version": "1.0",
    "icons":
    {
        "16": "assets/img/icon16.png",
        "48": "assets/img/icon48.png",
        "128": "assets/img/icon128.png"
    },
    "content_scripts":
    [
        {
            "matches": ["https://www.youtube.com/channel/*", "https://www.youtube.com/user/*"],
            "js": ["assets/js/jquery.js", "assets/js/content.js"]
        }
    ]
}